@using AUF.EMR.Application.Utilities;
@using AUF.EMR.Domain.Models.Enums.FamilyPlanning;
@model CreateFamilyPlanningVM

@{
    Layout = "Layouts/_FormLayout";
    ViewData["Title"] = "Create";
}

<div class="container mx-auto px-4 py-8">
    <div class="bg-white shadow-md rounded-lg p-8 mb-8">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="block mb-2 text-sm text-red-700"></div>
            <h1 class="text-xl md:text-3xl font-bold text-gray-800 mb-4">Add Family Planning</h1>
            <hr class="mb-4">
            <div class="grid grid-cols-1 md:grid-cols-2 gap-12">
                <div>
                    <div class="mb-4">
                        <label asp-for="FPRecord.ClientId" class="block mb-2 text-sm font-medium text-gray-700">ID</label>
                        <input asp-for="FPRecord.ClientId" class="bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-500 focus:border-blue-500 block w-full md:w-4/5 p-2.5" />
                        <span asp-validation-for="FPRecord.ClientId" class="block mb-2 text-sm text-red-700"></span>
                    </div>
                    <div class="mb-4">
                        <label asp-for="FPRecord.PhilhealthNo" class="block mb-2 text-sm font-medium text-gray-700">Philhealth No.</label>
                        <input asp-for="FPRecord.PhilhealthNo" class="bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-500 focus:border-blue-500 block w-full md:w-4/5 p-2.5" />
                        <span asp-validation-for="FPRecord.PhilhealthNo" class="block mb-2 text-sm text-red-700"></span>
                    </div>
                    <div class="mb-4">
                        <label asp-for="FPRecord.ClientHouseholdMemberId" class="block mb-2 text-sm font-medium text-gray-700">Household Member ID</label>
                        <select asp-for="FPRecord.ClientHouseholdMemberId" class="bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-500 focus:border-blue-500 block w-full md:w-4/5 p-2.5" asp-items="@(new SelectList(Model.WomenHouseholdMember, nameof(HouseholdMember.Id), nameof(HouseholdMember.FormattedFullName)))"></select>
                        <input type="hidden" asp-for="WomenHouseholdMember" value="@Model.WomenHouseholdMember" />
                    </div>
                    <br /> <hr class="w-full md:w-4/5" /> 
                    <div class="mb-2 mt-8">
                        <h6 class="text-md font-bold tex
                        t-gray-800">Personal Information</h6>
                    </div>
                    <div class="mb-4">
                        <label asp-for="FPRecord.ClientAge" class="block mb-2 text-sm font-medium text-gray-700">Age</label>
                        <input asp-for="FPRecord.ClientAge" class="bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-500 focus:border-blue-500 block w-full md:w-4/5 p-2.5" />
                        <span asp-validation-for="FPRecord.ClientAge" class="block mb-2 text-sm text-red-700"></span>
                    </div>
                    <div class="mb-4">
                        <label asp-for="FPRecord.ClientOccupation" class="block mb-2 text-sm font-medium text-gray-700">Occupation</label>
                        <input asp-for="FPRecord.ClientOccupation" class="bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-500 focus:border-blue-500 block w-full md:w-4/5 p-2.5" />
                        <span asp-validation-for="FPRecord.ClientOccupation" class="block mb-2 text-sm text-red-700"></span>
                    </div>
                    <div class="mb-4">
                        <label asp-for="FPRecord.ContactNo" class="block mb-2 text-sm font-medium text-gray-700">Contact No.</label>
                        <input asp-for="FPRecord.ContactNo" class="bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-500 focus:border-blue-500 block w-full md:w-4/5 p-2.5" />
                        <span asp-validation-for="FPRecord.ContactNo" class="block mb-2 text-sm text-red-700"></span>
                    </div>
                    <div class="mb-4">
                        <label asp-for="FPRecord.CivilStatus" class="block mb-2 text-sm font-medium text-gray-700">Civil Status</label>
                        <input asp-for="FPRecord.CivilStatus" class="bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-500 focus:border-blue-500 block w-full md:w-4/5 p-2.5" />
                        <span asp-validation-for="FPRecord.CivilStatus" class="block mb-2 text-sm text-red-700"></span>
                    </div>
                    <div class="mb-4">
                        <label asp-for="FPRecord.Religion" class="block mb-2 text-sm font-medium text-gray-700">Religion</label>
                        <input asp-for="FPRecord.Religion" class="bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-500 focus:border-blue-500 block w-full md:w-4/5 p-2.5" />
                        <span asp-validation-for="FPRecord.Religion" class="block mb-2 text-sm text-red-700"></span>
                    </div>
                    <br /> <hr class="w-full md:w-4/5" />
                    <div class="mb-2 mt-8">
                        <h6 class="text-md font-bold text-gray-800">Spouse's Information</h6>
                    </div>
                    <div class="mb-4">
                        <label asp-for="FPRecord.SpouseLastName" class="block mb-2 text-sm font-medium text-gray-700">Spouse's Last Name</label>
                        <input asp-for="FPRecord.SpouseLastName" class="bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-500 focus:border-blue-500 block w-full md:w-4/5 p-2.5" />
                        <span asp-validation-for="FPRecord.SpouseLastName" class="block mb-2 text-sm text-red-700"></span>
                    </div>
                    <div class="mb-4">
                        <label asp-for="FPRecord.SpouseFirstName" class="block mb-2 text-sm font-medium text-gray-700">Spouse's First Name</label>
                        <input asp-for="FPRecord.SpouseFirstName" class="bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-500 focus:border-blue-500 block w-full md:w-4/5 p-2.5" />
                        <span asp-validation-for="FPRecord.SpouseFirstName" class="block mb-2 text-sm text-red-700"></span>
                    </div>
                    <div class="mb-4">
                        <label asp-for="FPRecord.SpouseMiddleInitial" class="block mb-2 text-sm font-medium text-gray-700">Spouse's Middle Initial</label>
                        <input asp-for="FPRecord.SpouseMiddleInitial" class="bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-500 focus:border-blue-500 block w-full md:w-4/5 p-2.5" />
                        <span asp-validation-for="FPRecord.SpouseMiddleInitial" class="block mb-2 text-sm text-red-700"></span>
                    </div>
                    <div class="mb-4">
                        <label asp-for="FPRecord.SpouseBirthday" class="block mb-2 text-sm font-medium text-gray-700">Spouse's Birthdate</label>
                        <input asp-for="FPRecord.SpouseBirthday" class="bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-500 focus:border-blue-500 block w-full md:w-4/5 p-2.5" />
                        <span asp-validation-for="FPRecord.SpouseBirthday" class="block mb-2 text-sm text-red-700"></span>
                    </div>
                    <div class="mb-4">
                        <label asp-for="FPRecord.SpouseOccupation" class="block mb-2 text-sm font-medium text-gray-700">Spouse's Occupation</label>
                        <input asp-for="FPRecord.SpouseOccupation" class="bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-500 focus:border-blue-500 block w-full md:w-4/5 p-2.5" />
                        <span asp-validation-for="FPRecord.SpouseOccupation" class="block mb-2 text-sm text-red-700"></span>
                    </div>
                    <br /> <hr class="w-full md:w-4/5" /> 
                    <div class="mb-2 mt-8">
                        <h6 class="text-md font-bold text-gray-800">Children Planning</h6>
                    </div>
                    <div class="mb-4">
                        <label asp-for="FPRecord.LivingChildrenNo" class="block mb-2 text-sm font-medium text-gray-700">No. of Living Children</label>
                        <input asp-for="FPRecord.LivingChildrenNo" class="bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-500 focus:border-blue-500 block w-full md:w-4/5 p-2.5" />
                        <span asp-validation-for="FPRecord.LivingChildrenNo" class="block mb-2 text-sm text-red-700"></span>
                    </div>
                    <div class="mb-4">
                        <label class="block mb-2 text-sm font-medium text-gray-700">Plan to have more children?</label>
                        <div class="flex">
                            <div class="flex items-center me-4">
                                <input asp-for="FPRecord.IsPlanningChildren" value="true" type="radio" class="w-4 h-4 text-blue-600 bg-gray-50 border-gray-300 focus:ring-blue-500">
                                <label asp-for="FPRecord.IsPlanningChildren" class="ms-2 text-sm font-medium text-gray-900">Yes</label>
                            </div>
                            <div class="flex items-center me-4">
                                <input asp-for="FPRecord.IsPlanningChildren" value="false" type="radio" class="w-4 h-4 text-blue-600 bg-gray-50 border-gray-300 focus:ring-blue-500">
                                <label asp-for="FPRecord.IsPlanningChildren" class="ms-2 text-sm font-medium text-gray-900">No</label>
                            </div>
                        </div>
                        <span asp-validation-for="FPRecord.IsPlanningChildren" class="block mb-2 text-sm text-red-700"></span>
                    </div>
                    <div class="mb-4">
                        <label asp-for="FPRecord.AverageMonthlyIncome" class="block mb-2 text-sm font-medium text-gray-700">Average Monthly Income</label>
                        <input asp-for="FPRecord.AverageMonthlyIncome" class="bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-500 focus:border-blue-500 block w-full md:w-4/5 p-2.5" />
                        <span asp-validation-for="FPRecord.AverageMonthlyIncome" class="block mb-2 text-sm text-red-700"></span>
                    </div>
                    <br /> <hr class="w-full md:w-4/5" /> 
                    <div class="mb-2 mt-8">
                        <h6 class="text-md font-bold text-gray-800">Type</h6>
                    </div>
                    <div class="mb-4">
                        <label class="block mb-2 text-sm font-medium text-gray-700">Plan to have more children?</label>
                        <div class="flex">
                            <div class="flex items-center me-4">
                                <input asp-for="FPRecord.ClientType.IsCurrentUser" value="true" type="radio" class="w-4 h-4 text-blue-600 bg-gray-50 border-gray-300 focus:ring-blue-500">
                                <label asp-for="FPRecord.IsPlanningChildren" class="ms-2 text-sm font-medium text-gray-900">Current User</label>
                            </div>
                            <div class="flex items-center me-4">
                                <input asp-for="FPRecord.ClientType.IsCurrentUser" value="false" type="radio" class="w-4 h-4 text-blue-600 bg-gray-50 border-gray-300 focus:ring-blue-500">
                                <label asp-for="FPRecord.ClientType.IsCurrentUser" class="ms-2 text-sm font-medium text-gray-900">New Acceptor</label>
                            </div>
                        </div>
                        <span asp-validation-for="FPRecord.ClientType.IsCurrentUser" class="block mb-2 text-sm text-red-700"></span>
                    </div>
                    <div class="mb-4">
                        <label asp-for="ClientType.CurrentUserType" class="block mb-2 text-sm font-medium text-gray-700">Current User Type</label>
                        <select asp-for="ClientType.CurrentUserType" class="bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-500 focus:border-blue-500 block w-full md:w-4/5 p-2.5">
                            <option selected="selected" value="">Please select</option>
                            @foreach (var value in Enum.GetValues(typeof(CurrentUserType)))
                            {
                                <option value="@value">@EnumExtension.GetEnumTitleCase((CurrentUserType)value)</option>
                            }
                        </select>
                        <span asp-validation-for="ClientType.CurrentUserType" class="block mb-2 text-sm text-red-700"></span>
                    </div>
                    <div class="mb-4">
                        <label asp-for="ClientType.ReasonForFP" class="block mb-2 text-sm font-medium text-gray-700">Reason for FP</label>
                        <select asp-for="ClientType.ReasonForFP" class="bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-500 focus:border-blue-500 block w-full md:w-4/5 p-2.5">
                            <option selected="selected" value="">Please select</option>
                            @foreach (var value in Enum.GetValues(typeof(ReasonForFP)))
                            {
                                <option value="@value">@EnumExtension.GetEnumTitleCase((ReasonForFP)value)</option>
                            }
                        </select>
                        <span asp-validation-for="ClientType.ReasonForFP" class="block mb-2 text-sm text-red-700"></span>
                    </div>
                    <div class="mb-4">
                        <label asp-for="ClientType.ReasonOthers" class="block mb-2 text-sm font-medium text-gray-700">Other Reasons</label>
                        <input asp-for="ClientType.ReasonOthers" class="bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-500 focus:border-blue-500 block w-full md:w-4/5 p-2.5" />
                        <span asp-validation-for="ClientType.ReasonOthers" class="block mb-2 text-sm text-red-700"></span>
                    </div>
                    <div class="mb-4 ">
                        <label class="block mb-2 text-sm font-medium text-gray-700">Is Reason Medical?</label>
                        <div class="form-check form-check-inline">
                            <input class="rounded-full" type="radio" asp-for="ClientType.IsReasonMedical" value="true" />
                            <label class="block mb-2 text-sm font-medium text-gray-700">Yes</label>
                        </div>
                        <div class="form-check form-check-inline">
                            <input class="rounded-full" type="radio" asp-for="ClientType.IsReasonMedical" value="false" />
                            <label class="block mb-2 text-sm font-medium text-gray-700">No</label>
                        </div>
                    </div>
                    <div class="mb-4 form-check">
                        <label class="block mb-2 text-sm font-medium text-gray-700">
                            <input class="border border-gray-400 rounded-sm" asp-for="ClientType.IsReasonSideEffects" /> @Html.DisplayNameFor(model => model.ClientType.IsReasonSideEffects)
                        </label>
                    </div>
                    <div class="mb-4">
                        <label asp-for="ClientType.ReasonMethodText" class="block mb-2 text-sm font-medium text-gray-700">Method Reason/s</label>
                        <input asp-for="ClientType.ReasonMethodText" class="bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-500 focus:border-blue-500 block w-full md:w-4/5 p-2.5" />
                        <span asp-validation-for="ClientType.ReasonMethodText" class="block mb-2 text-sm text-red-700"></span>
                    </div>
                    <div class="mb-4 form-check">
                        <label class="block mb-2 text-sm font-medium text-gray-700">
                            <input class="border border-gray-400 rounded-sm" asp-for="ClientType.IsMethodImplant" /> @Html.DisplayNameFor(model => model.ClientType.IsMethodImplant)
                        </label>
                    </div>
                    <div class="mb-4 form-check">
                        <label class="block mb-2 text-sm font-medium text-gray-700">
                            <input class="border border-gray-400 rounded-sm" asp-for="ClientType.IsMethodInjectable" /> @Html.DisplayNameFor(model => model.ClientType.IsMethodInjectable)
                        </label>
                    </div>
                    <div class="mb-4 form-check">
                        <label class="block mb-2 text-sm font-medium text-gray-700">
                            <input class="border border-gray-400 rounded-sm" asp-for="ClientType.IsMethodLAM" /> @Html.DisplayNameFor(model => model.ClientType.IsMethodLAM)
                        </label>
                    </div>
                    <div class="mb-4 form-check">
                        <label class="block mb-2 text-sm font-medium text-gray-700">
                            <input class="border border-gray-400 rounded-sm" asp-for="ClientType.IsMethodIUD" /> @Html.DisplayNameFor(model => model.ClientType.IsMethodIUD)
                        </label>
                    </div>
                    <div class="mb-4 form-check">
                        <label class="block mb-2 text-sm font-medium text-gray-700">
                            <input class="border border-gray-400 rounded-sm" asp-for="ClientType.IsMethodCOC" /> @Html.DisplayNameFor(model => model.ClientType.IsMethodCOC)
                        </label>
                    </div>
                    <div class="mb-4 form-check">
                        <label class="block mb-2 text-sm font-medium text-gray-700">
                            <input class="border border-gray-400 rounded-sm" asp-for="ClientType.IsMethodSDM" /> @Html.DisplayNameFor(model => model.ClientType.IsMethodSDM)
                        </label>
                    </div>
                    <div class="mb-4 form-check">
                        <label class="block mb-2 text-sm font-medium text-gray-700">
                            <input class="border border-gray-400 rounded-sm" asp-for="ClientType.IsMethodBTL" /> @Html.DisplayNameFor(model => model.ClientType.IsMethodBTL)
                        </label>
                    </div>
                    <div class="mb-4 form-check">
                        <label class="block mb-2 text-sm font-medium text-gray-700">
                            <input class="border border-gray-400 rounded-sm" asp-for="ClientType.IsMethodPOP" /> @Html.DisplayNameFor(model => model.ClientType.IsMethodPOP)
                        </label>
                    </div>
                    <div class="mb-4 form-check">
                        <label class="block mb-2 text-sm font-medium text-gray-700">
                            <input class="border border-gray-400 rounded-sm" asp-for="ClientType.IsMethodBBT" /> @Html.DisplayNameFor(model => model.ClientType.IsMethodBBT)
                        </label>
                    </div>
                    <div class="mb-4 form-check">
                        <label class="block mb-2 text-sm font-medium text-gray-700">
                            <input class="border border-gray-400 rounded-sm" asp-for="ClientType.IsMethodNSV" /> @Html.DisplayNameFor(model => model.ClientType.IsMethodNSV)
                        </label>
                    </div>
                    <div class="mb-4 form-check">
                        <label class="block mb-2 text-sm font-medium text-gray-700">
                            <input class="border border-gray-400 rounded-sm" asp-for="ClientType.IsMethodCondom" /> @Html.DisplayNameFor(model => model.ClientType.IsMethodCondom)
                        </label>
                    </div>
                    <div class="mb-4 form-check">
                        <label class="block mb-2 text-sm font-medium text-gray-700">
                            <input class="border border-gray-400 rounded-sm" asp-for="ClientType.IsMethodBOM" /> @Html.DisplayNameFor(model => model.ClientType.IsMethodBOM)
                        </label>
                    </div>
                    <br /> <hr class="w-full md:w-4/5" /> 
                    <div class="mb-2 mt-8">
                        <h6 class="text-md font-bold text-gray-800">Medical History</h6>
                    </div>
                    <div class="mb-4 form-check">
                        <label class="block mb-2 text-sm font-medium text-gray-700">
                            <input class="border border-gray-400 rounded-sm" asp-for="MedicalHistory.HasSevereHeadache" /> @Html.DisplayNameFor(model => model.MedicalHistory.HasSevereHeadache)
                        </label>
                    </div>
                    <div class="mb-4 form-check">
                        <label class="block mb-2 text-sm font-medium text-gray-700">
                            <input class="border border-gray-400 rounded-sm" asp-for="MedicalHistory.HasHeartAttack" /> @Html.DisplayNameFor(model => model.MedicalHistory.HasHeartAttack)
                        </label>
                    </div>
                    <div class="mb-4 form-check">
                        <label class="block mb-2 text-sm font-medium text-gray-700">
                            <input class="border border-gray-400 rounded-sm" asp-for="MedicalHistory.HasHematoma" /> @Html.DisplayNameFor(model => model.MedicalHistory.HasHematoma)
                        </label>
                    </div>
                    <div class="mb-4 form-check">
                        <label class="block mb-2 text-sm font-medium text-gray-700">
                            <input class="border border-gray-400 rounded-sm" asp-for="MedicalHistory.HasBreastCancer" /> @Html.DisplayNameFor(model => model.MedicalHistory.HasBreastCancer)
                        </label>
                    </div>
                    <div class="mb-4 form-check">
                        <label class="block mb-2 text-sm font-medium text-gray-700">
                            <input class="border border-gray-400 rounded-sm" asp-for="MedicalHistory.HasChestPain" /> @Html.DisplayNameFor(model => model.MedicalHistory.HasChestPain)
                        </label>
                    </div>
                    <div class="mb-4 form-check">
                        <label class="block mb-2 text-sm font-medium text-gray-700">
                            <input class="border border-gray-400 rounded-sm" asp-for="MedicalHistory.HasCough" /> @Html.DisplayNameFor(model => model.MedicalHistory.HasCough)
                        </label>
                    </div>
                    <div class="mb-4 form-check">
                        <label class="block mb-2 text-sm font-medium text-gray-700">
                            <input class="border border-gray-400 rounded-sm" asp-for="MedicalHistory.HasJaundice" /> @Html.DisplayNameFor(model => model.MedicalHistory.HasJaundice)
                        </label>
                    </div>
                    <div class="mb-4 form-check">
                        <label class="block mb-2 text-sm font-medium text-gray-700">
                            <input class="border border-gray-400 rounded-sm" asp-for="MedicalHistory.HasVaginalBleeding" /> @Html.DisplayNameFor(model => model.MedicalHistory.HasVaginalBleeding)
                        </label>
                    </div>
                    <div class="mb-4 form-check">
                        <label class="block mb-2 text-sm font-medium text-gray-700">
                            <input class="border border-gray-400 rounded-sm" asp-for="MedicalHistory.HasAbnormalDischarge" /> @Html.DisplayNameFor(model => model.MedicalHistory.HasAbnormalDischarge)
                        </label>
                    </div>
                    <div class="mb-4 form-check">
                        <label class="block mb-2 text-sm font-medium text-gray-700">
                            <input class="border border-gray-400 rounded-sm" asp-for="MedicalHistory.HasTakenRifampicin" /> @Html.DisplayNameFor(model => model.MedicalHistory.HasTakenRifampicin)
                        </label>
                    </div>
                    <div class="mb-4 form-check">
                        <label class="block mb-2 text-sm font-medium text-gray-700">
                            <input class="border border-gray-400 rounded-sm" asp-for="MedicalHistory.IsSmoker" /> @Html.DisplayNameFor(model => model.MedicalHistory.IsSmoker)
                        </label>
                    </div>
                    <div class="mb-4 form-check">
                        <label class="block mb-2 text-sm font-medium text-gray-700">
                            <input class="border border-gray-400 rounded-sm" asp-for="MedicalHistory.HasDisability" /> @Html.DisplayNameFor(model => model.MedicalHistory.HasDisability)
                        </label>
                    </div>
                    <div class="mb-4">
                        <label asp-for="MedicalHistory.Disability" class="block mb-2 text-sm font-medium text-gray-700">Disability</label>
                        <input asp-for="MedicalHistory.Disability" class="bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-500 focus:border-blue-500 block w-full md:w-4/5 p-2.5" />
                        <span asp-validation-for="MedicalHistory.Disability" class="block mb-2 text-sm text-red-700"></span>
                    </div>
                    <div class="mb-2 mt-8">
                        <h6 class="text-md font-bold text-gray-800">Obstetrical History</h6>
                    </div>
                    <div class="mb-4">
                        <label asp-for="ObstetricalHistory.NumGravidityPregnancies" class="block mb-2 text-sm font-medium text-gray-700">No. of Gravidity Pregnancies</label>
                        <input asp-for="ObstetricalHistory.NumGravidityPregnancies" class="bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-500 focus:border-blue-500 block w-full md:w-4/5 p-2.5" />
                        <span asp-validation-for="ObstetricalHistory.NumGravidityPregnancies" class="block mb-2 text-sm text-red-700"></span>
                    </div>
                    <div class="mb-4">
                        <label asp-for="ObstetricalHistory.NumParityPregnancies" class="block mb-2 text-sm font-medium text-gray-700">No. of Parity Pregnancies</label>
                        <input asp-for="ObstetricalHistory.NumParityPregnancies" class="bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-500 focus:border-blue-500 block w-full md:w-4/5 p-2.5" />
                        <span asp-validation-for="ObstetricalHistory.NumParityPregnancies" class="block mb-2 text-sm text-red-700"></span>
                    </div>
                    <div class="mb-4">
                        <label asp-for="ObstetricalHistory.NumFullTerm" class="block mb-2 text-sm font-medium text-gray-700">No. of Full Term</label>
                        <input asp-for="ObstetricalHistory.NumFullTerm" class="bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-500 focus:border-blue-500 block w-full md:w-4/5 p-2.5" />
                        <span asp-validation-for="ObstetricalHistory.NumFullTerm" class="block mb-2 text-sm text-red-700"></span>
