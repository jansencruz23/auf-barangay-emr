@using AUF.EMR.MVC.Models.PrintVM;
@model PrintAdultsMasterlistVM

@{
    ViewData["Barangay"] = Model.Barangay.BarangayName;
}

<h6>Masterlist of Adults 20-59 years old</h6>
@foreach (var member in Model.Adults)
{
    <p>
        @member.HouseholdNo -
        @member.FullName -
        @member.Household.FullAddress -
        @member.Sex -
        @member.Age -
        @member.Birthday -
        @(member.IsNhts == null ? "NHTS not set" : member.IsNhts.Value ? "NHTS" : "Non NHTS")
    </p>
}